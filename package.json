{
  "name": "minesweeper-game",
  "description": "File based routing and MDX support Vite.js tooling. Cloudflare Pages.",
  "scripts": {
    "build": "npm run typecheck && npm run build.client && npm run build.ssr",
    "build.client": "vite build",
    "build.ssr": "vite build --ssr src/entry.cloudflare.tsx",
    "dev": "vite",
    "dev.debug": "node --inspect-brk node_modules/vite/bin/vite.js --mode ssr",
    "dev.ssr": "node --inspect node_modules/vite/bin/vite.js --mode ssr",
    "lint": "eslint \"src/**/*.ts*\"",
    "serve": "wrangler pages dev ./dist",
    "start": "npm run dev",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.0.34-dev20220701233003",
    "@builder.io/qwik-city": "0.0.16",
    "@types/eslint": "8.4.5",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "eslint": "8.18.0",
    "eslint-plugin-qwik": "0.0.34-dev20220701233003",
    "node-fetch": "2.6.7",
    "typescript": "4.7.4",
    "vite": "2.9.13",
    "vite-tsconfig-paths": "3.5.0",
    "wrangler": "beta"
  },
  "engines": {
    "node": ">=14"
  },
  "homepage": "https://qwik.builder.io/",
  "license": "",
  "private": true
}
